<mjml>
  <mj-head>
    <mj-title> AXA in partnership with Singtel - Dominic Arrojado </mj-title>

    <!-- START of Global Attributes -->
    <mj-breakpoint width="800px" />
    <mj-attributes>
      <mj-all padding="0" font-family="Arial" line-height="1.4" />
    </mj-attributes>
    <!-- END of Global Attributes -->

    <!-- START of Inline Style Overrides -->
    <!-- prettier-ignore -->
    <mj-style inline="inline">
      .inline-img {
        display: inline;
        outline: none;
        border: 0;
        font-size: 13px;
        text-decoration: none;
      }

      .td-axa,
      .td-partner,
      .td-singtel {
        vertical-align: bottom;
      }

      .td-partner {
        text-align: right;
        padding: 0 10px 20px;
      }

      .td-axa a,
      .td-singtel a {
        display: block;
      }

      .td-axa,
      .axa-img {
        width: 71px;
      }

      .td-singtel,
      .singtel-img {
        width: 110px;
      }

      .axa-img,
      .singtel-img {
        display: block;
        height: auto;
      }

      .td-num,
      .td-desc {
        vertical-align: top;
      }

      .td-num {
        width: 15px;
      }

      .btn-text,
      .btn-text-underline {
        display: inline-block;
        margin: 0;
        background: transparent;
        font-family: Arial;
        font-size: 11px;
        line-height: 1.4;
        text-decoration: none;
        cursor: pointer;
      }

      .btn-text {
        color: #ffffff;
        text-decoration: none;
      }

      .btn-text-underline {
        color: #777777;
        text-decoration: underline;
      }

      .social-facebook-img {
        width: 72px;
        height: auto;
      }

      .social-youtube-img,
      .social-linkedin-img {
        width: 31px;
        height: auto;
      }
    </mj-style>
    <!-- END of Inline Style Overrides -->

    <!-- START of Internal/Mobile Styles -->
    <!-- prettier-ignore -->
    <mj-style>
      .btn-text-hover a:hover,
      a.btn-text-hover:hover {
        cursor: pointer !important;
        text-decoration: underline !important;
      }

      .btn-hover-white a:hover {
        background-color: #ffffff !important;
        color: #133483 !important;
      }

      .btn-hover-blue a:hover {
        background-color: #133483 !important;
        color: #ffffff !important;
      }

      @media only screen and (max-width: 799px) {
        .wrapper > table > tbody > tr > td {
          padding-top: 0 !important;
          padding-bottom: 0 !important;
        }

        .td-partner {
          font-size: 14px !important;
        }

        .col-right-text {
          padding-right: 0 !important;
          padding-left: 0 !important;
        }

        .col-right-text,
        .col-right-text > div {
          text-align: center !important;
        }

        .col-right-text > table {
          margin: 0 auto !important;
        }
      }
    </mj-style>
    <!-- END of Internal/Mobile Styles -->

