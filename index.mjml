  <mj-body width="800px" background-color="#d7e1e9">
    <mj-wrapper css-class="wrapper" padding="40px 0">
      <!-- START of View In Browser -->
      <mj-section>
        <mj-group>
          <mj-column
            width="100%"
            background-color="#343c3f"
            padding="15px 20px"
          >
            <mj-button
              href="#"
              css-class="btn-text-hover"
              align="center"
              background-color="transparent"
              inner-padding="0"
              color="#ffffff"
              font-size="13px"
            >
              Click here to view this email in your browser
            </mj-button>
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of View In Browser -->

      <!-- START of Brand -->
      <mj-section background-color="#ffffff" padding="15px">
        <mj-group>
          <mj-column width="100%">
            <mj-table color="#999999" font-size="17px">
              <tr>
                <td class="td-axa">
                  <a
                    href="https://www.axa.com.sg/"
                    target="_blank"
                    rel="noopener noreferrer nofollow"
                    class="btn-text"
                  >
                    <img
                      src="images/logo-axa.png"
                      alt="AXA"
                      class="inline-img axa-img"
                    />
                  </a>
                </td>
                <td class="td-partner">In partnership with:</td>
                <td class="td-singtel">
                  <a
                    href="https://www.singtel.com/"
                    target="_blank"
                    rel="noopener noreferrer nofollow"
                    class="btn-text"
                  >
                    <img
                      src="images/logo-singtel.png"
                      alt="Singtel"
                      class="inline-img singtel-img"
                    />
                  </a>
                </td>
              </tr>
            </mj-table>
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of Brand -->

      <!-- START of Hero Image 1 -->
      <mj-section>
        <mj-group>
          <mj-column width="100%">
            <mj-image
              src="images/banner-axa-travel-insurance.png"
              alt="AXA 50% OFF* Single Trip Travel Insurance, in the background is a family on vacation"
            />
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of Hero Image 1 -->

      <!-- START of Content 1 -->
      <mj-section background-color="#133483" padding="50px 20px">
        <mj-group>
          <mj-column width="100%">
            <mj-text
              align="center"
              color="#ffffff"
              font-family="Georgia, Arial"
              font-size="37px"
              font-weight="bold"
            >
              Pay Less, Worry Less
            </mj-text>
            <mj-text
              align="center"
              padding-top="20px"
              color="#ffffff"
              font-size="16px"
            >
              This Great Singapore Sale, AXA SmartTraveller Single Trip Travel
              Insurance
            </mj-text>
            <mj-text align="center" color="#ffffff" font-size="16px">
              lets you explore the world with a greater peace of mind
            </mj-text>
            <mj-text align="center" color="#ffffff" font-size="16px">
              at 50% off until 30 June 2017.
            </mj-text>
            <mj-button
              href="https://www.axa.com.sg/travel-insurance/smarttraveller"
              rel="noopener noreferrer nofollow"
              css-class="btn-hover-white"
              align="center"
              padding-top="20px"
              border="2px solid #ffffff"
              border-radius="0"
              background-color="transparent"
              inner-padding="9px 50px"
              color="#ffffff"
              font-size="16px"
              font-weight="bold"
              text-transform="uppercase"
            >
              Buy Now
            </mj-button>
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of Content 1 -->

      <!-- START of Content 2 -->
      <mj-section background-color="#f5f5f5" padding="40px 20px 60px">
        <mj-column width="35%" padding-top="40px">
          <mj-image
            align="center"
            src="images/icon-globe.png"
            alt="globe"
            width="243px"
          />
        </mj-column>
        <mj-column width="65%">
          <mj-text
            css-class="col-right-text"
            padding="20px 30px 0"
            color="#2e2e2e"
            font-family="Georgia, Arial"
            font-size="32px"
            font-weight="bold"
            line-height="1.2"
          >
            Get up to 40%* Off Annual Travel Insurance. Singtel Users Get
            Additional 8%^ Off!
          </mj-text>
          <mj-text
            css-class="col-right-text"
            padding="20px 30px 0"
            color="#2e2e2e"
            font-size="16px"
          >
            Live out of your suitcase with AXA SmartTraveller Annual Travel
            Insurance. Singtel users enjoy an exclusive additional 8% off with
            our promo code.
          </mj-text>
          <mj-image
            css-class="col-right-text"
            align="left"
            padding="20px 30px 0"
            src="images/code.png"
            alt="Enter AXAextra8 upon checkout"
            width="252px"
          />
          <mj-text
            css-class="col-right-text"
            padding="10px 30px 0"
            color="#2e2e2e"
            font-size="16px"
          >
            Promotion lasts until 20 June 2017.
          </mj-text>
          <mj-button
            href="https://www.axa.com.sg/travel-insurance/smarttraveller"
            rel="noopener noreferrer nofollow"
            css-class="col-right-text btn-hover-blue"
            align="left"
            padding="20px 30px 0"
            border="2px solid #133483"
            border-radius="0"
            background-color="transparent"
            inner-padding="9px 50px"
            color="#133483"
            font-size="16px"
            font-weight="bold"
            text-transform="uppercase"
          >
            Buy Now
          </mj-button>
        </mj-column>
      </mj-section>
      <!-- END of Content 2 -->

      <!-- START of Hero Image 2 -->
      <mj-section>
        <mj-group>
          <mj-column width="100%">
            <mj-image
              src="images/banner-axa-smart-home.png"
              alt="AXA 40% OFF* SmartHome Insurance, in the background is a family in the kitchen and kids playing"
            />
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of Hero Image 2 -->

      <!-- START of Content 3 -->
      <mj-section background-color="#f5f5f5" padding="50px 20px">
        <mj-group>
          <mj-column width="100%">
            <mj-text
              align="center"
              color="#2e2e2e"
              font-family="Georgia, Arial"
              font-size="37px"
              font-weight="bold"
